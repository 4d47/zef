{
    "perl"        : "v6.0.0",
    "name"        : "Zef",
    "version"     : "*",
    "description" : "CPAN but better. Maybe?",
    "depends"     : [
        "JSON::Tiny",
        "IO::Socket::SSL",
        "MIME::Base64",
        "HTTP::UserAgent"
    ],
    "provides"    : {
        "Zef" : "lib/Zef.pm6",

        "Zef::App" : "lib/Zef/App.pm6",

        "Zef::Exception" : "lib/Zef/Exception.pm6",

        "Zef::Getter"          : "lib/Zef/Getter.pm6",
        "Zef::Phase::Getting"  : "lib/Zef/Phase/Getting.pm6",
        "Zef::Plugin::Git"     : "lib/Zef/Plugin/Git.pm6",
